ocalize value={income-expense} options={{style: 'currency', currency: 'USD', minimumFractionDigits: 2, maximumFractionDigits: 2}}/> USD\n                        </td>\n                    </tr>\n                    </tbody>\n                </table>\n            </div>\n        );\n    }\n}\n\nexport default Total;\n"]},"sourceType":"script","hash":"ecddee42b5b35b166d80f02e538c0aef2d0950e9"}
