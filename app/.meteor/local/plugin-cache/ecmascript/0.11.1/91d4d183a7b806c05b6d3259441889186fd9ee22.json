tar avatar={information.avatar}\n                                                          name={(information.name || \"\") + \" \" + (information.lastName || \"\")}\n                                                          size=\"50\"/>\n                                            <div className=\"title\" style={titleStyle}>\n                                                {(information.name || \"\") + \" \" + (information.lastName || \"\")}\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div className=\"col-md-6\">\n                                        <div className=\"form-group\">\n                                            <label htmlFor=\"informationName\">\n                                                <Translate value={getTranslation(\"settings.form.accountIs.title\")}/>\n                                            </label>\n                                            <div className=\"form-control-static\">\n                                                <OverlayTrigger placement=\"top\" overlay={getTooltip(<Translate value={getTranslation(\"settings.form.accountIs.brand\")} />)}>\n                                                    <div className={\"account-type-icon \"+ (settings.type.brand?(settings.type.influencer?\"both\":\"brand\"):\"not\")}>\n                                                        {settings.type.brand?<Translate value={getTranslation(\"settings.form.accountIs.badges.brand\")}/>:<i className=\"wb-close\"/>}\n                                                        <div className=\"name\">\n                                                            <Translate value={getTranslation(\"settings.form.accountIs.brand\")} />\n                                                        </div>\n                                                    </div>\n                                                </OverlayTrigger>\n                                                <OverlayTrigger placement=\"top\" overlay={getTooltip(<Translate value={getTranslation(\"settings.form.accountIs.influencer\")} />)}>\n                                                    <div className={\"account-type-icon \"+ (settings.type.influencer?(settings.type.brand?\"both\":\"influencer\"):\"not\")}>\n                                                        {settings.type.influencer?<Translate value={getTranslation(\"settings.form.accountIs.badges.influencer\")}/>:<i className=\"wb-close\"/>}\n                                                        <div className=\"name\">\n                                                            <Translate value={getTranslation(\"settings.form.accountIs.influencer\")} />\n                                                        </div>\n                                                    </div>\n                                                </OverlayTrigger>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                            <div className=\"col-md-5 \">\n                                <h4><Translate value={getTranslation(\"summary.information.campaigns.title\")}/></h4>\n                                <div className=\"row campaigns\">\n                                    <div className=\"col-md-4\">\n                                        <div className=\"count\">\n                                            19\n                                            <div className=\"title\">\n                                                <Translate value={getTranslation(\"summary.information.campaigns.label.total\")}/>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div className=\"col-md-4\">\n                                        <div className=\"count\">\n                                            19\n                                            <div className=\"title\">\n                                                <Translate value={getTranslation(\"summary.information.campaigns.label.created\")}/>\n                                            </div>\n                                        </div>\n                                    </div>\n                                    <div className=\"col-md-4\">\n                                        <div className=\"count\">\n                                            19\n                                            <div className=\"title\">\n                                                <Translate value={getTranslation(\"summary.information.campaigns.label.partOf\")}/>\n                                            </div>\n                                        </div>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                        <div className=\"row\">\n                            <div className=\"col-md-4\">\n                                <h4><Translate value={getTranslation(\"summary.information.areas\")}/></h4>\n                                <div className=\"category-list\">\n\n                                </div>\n                            </div>\n                            <div className=\"col-md-4\">\n                                <h4><Translate value={getTranslation(\"summary.information.categories\")}/></h4>\n                                <div className=\"category-list\">\n\n                                </div>\n                            </div>\n                            <div className=\"col-md-4\">\n                                <h4><Translate value={getTranslation(\"summary.information.specialty\")}/></h4>\n                                <div className=\"category-list\">\n\n                                </div>\n                            </div>\n                            {/*\n                            <div className=\"col-md-4\">\n                                <h4><Translate value={getTranslation(\"summary.information.groups\")}/></h4>\n                                <div className=\"group-list\">\n                                    {groups.map((g, i)=>(\n                                        <div className=\"group\" key={i}>{g}</div>\n                                    ))}\n                                </div>\n                            </div>\n                            */}\n                        </div>\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default SummaryInformation;\n"]},"sourceType":"script","hash":"91d4d183a7b806c05b6d3259441889186fd9ee22"}
