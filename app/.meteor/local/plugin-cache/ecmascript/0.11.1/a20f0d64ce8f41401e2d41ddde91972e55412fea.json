\n\nvar makePascalCased = function (word) {\n  return word[0].toUpperCase() + word.slice(1).toLowerCase();\n};\n\nMeteor.connectWith = function (service, options, callback) {\n  // Support a callback without options\n  if (!callback && typeof options === \"function\") {\n    callback = options;\n    options = null;\n  }\n\n  var connectCredentialRequestCompleteCallback = Accounts.oauth.connectCredentialRequestCompleteHandler(callback);\n  var Service;\n\n  if (service === \"instagram\") {\n    Service = Package[\"bozhao:accounts-instagram\"].Instagram;\n  } else {\n    Service = typeof service === \"string\" ? Package[service + \"-oauth\"][makePascalCased(service)] : service;\n  }\n\n  Service.requestCredential(options, connectCredentialRequestCompleteCallback);\n};","map":{"version":3,"sources":["imports/modules/accounts/client/social-connect.jsx"],"names":["Meteor","module","watch","require","v","Accounts","oauth","tryConnectAfterPopupClosed","credentialToken","callback","credentialSecret","OAuth","_retrieveCredentialSecret","options","call","error","res","connectCredentialRequestCompleteHandler","credentialTokenOrError","Error","makePascalCased","word","toUpperCase","slice","toLowerCase","connectWith","service","connectCredentialRequestCompleteCallback","Service","Package","Instagram","requestCredential"],"mappings":"AAAA,IAAIA,MAAJ;AAAWC,OAAOC,KAAP,CAAaC,QAAQ,eAAR,CAAb,EAAsC;AAACH,QAAD,YAAQI,CAAR,EAAU;AAACJ,aAAOI,CAAP;AAAS;AAApB,CAAtC,EAA4D,CAA5D;;AAEXC,SAASC,KAAT,CAAeC,0BAAf,GAA4C,UAAUC,eAAV,EAA2BC,QAA3B,EAAqC;AAC7E;AACA,MAAIC,mBAAmBC,MAAMC,yBAAN,CAAgCJ,eAAhC,KAAoD,IAA3E;AACA,MAAIK,UAAU;AACVP,WAAO;AACHE,uBAAiBA,eADd;AAEHE,wBAAkBA;AAFf;AADG,GAAd,CAH6E,CAS7E;;AACAV,SAAOc,IAAP,CAAY,iBAAZ,EAA+BD,OAA/B,EAAwC,UAAUE,KAAV,EAAiBC,GAAjB,EAAsB;AAC1D;AACA,QAAIP,QAAJ,EAAc;AACVA,eAASM,KAAT,EAAgBC,GAAhB;AACH;AACJ,GALD;AAMH,CAhBD;;AAkBAX,SAASC,KAAT,CAAeW,uCAAf,GAAyD,UAAUR,QAAV,EAAoB;AACzE,SAAO,UAAUS,sBAAV,EAAkC;AACrC;AACA,QAAIA,0BAA0BA,kCAAkCC,KAAhE,EAAuE;AACnE,UAAIV,QAAJ,EAAc