\n    };\n    let client = Meteor.wrapAsync(Facebook.napi, Facebook);\n\n    try {\n      return client(`${adAccount}/campaigns`, \"post\", campaign);\n    } catch (e) {\n      console.log(\"ERROR\", e);\n      return {\n        error: JSON.parse(e.message).error.error_user_msg\n      };\n    }\n  }\n});","map":{"version":3,"sources":["imports/modules/facebook/server/marketing/campaign/create.jsx"],"names":["FacebookConnection","module","watch","require","default","v","Meteor","methods","accountID","adAccount","data","account","SocialAccounts","findOne","_id","connection","Facebook","accessToken","campaign","name","adlabels","budget_rebalance_flag","buying_type","buyingType","promoted_object","spend_cap","Number","spendCap","status","objective","client","wrapAsync","napi","e","console","log","error","JSON","parse","message","error_user_msg"],"mappings":"AAAA,IAAIA,kBAAJ;AAAuBC,OAAOC,KAAP,CAAaC,QAAQ,kBAAR,CAAb,EAAyC;AAACC,UAAQC,CAAR,EAAU;AAACL,yBAAmBK,CAAnB;AAAqB;;AAAjC,CAAzC,EAA4E,CAA5E;AAA+E,IAAIC,MAAJ;AAAWL,OAAOC,KAAP,CAAaC,QAAQ,eAAR,CAAb,EAAsC;AAACG,SAAOD,CAAP,EAAS;AAACC,aAAOD,CAAP;AAAS;;AAApB,CAAtC,EAA4D,CAA5D;AAGjHC,OAAOC,OAAP,CAAe;AACX,kCAAgC,UAAUC,SAAV,EAAqBC,SAArB,EAAgCC,IAAhC,EAAsC;AAClE,QAAIC,UAAUC,eAAeC,OAAf,CAAuB;AAACC,WAAKN;AAAN,KAAvB,CAAd;AACA,QAAIO,aAAaJ,QAAQI,UAAzB;AACA,QAAIC,WAAWhB,mBAAmBe,WAAWE,WAA9B,CAAf;AAEA,QAAIC,WAAS;AACTC,YAAMT,KAAKS,IADF;AAETC,gBAAS,EAFA;AAGTC,6BAAsB,IAHb;AAITC,mBAAaZ,KAAKa,UAJT;AAIqB;AAC9BC,uBAAiB,IALR;AAKc;AACvBC,iBAAWC,OAAOhB,KAAKiB,QAAZ,IAAsB,GANxB;AAOTC,cAAQ,QAPC;AAOS;AAClBC,iBAAWnB,KAAKmB,SARP,CAQiB;;AARjB,KAAb;AAWA,QAAIC,SAASxB,OAAOyB,SAAP,CAAiBf,SAASgB,IAA1B,EAAgChB,QAAhC,CAAb;;AACA,QAAG;AACC,aAAOc,OAAQ,GAAErB,SAAU,YAApB,EAAiC,MAAjC,EAAyCS,QAAzC,CAAP;AACH,KAFD,CAEC,OAAMe,CAAN,EAAQ;AACLC,cAAQC,GAAR,CAAY,OAAZ,EAAqBF,CAArB;AACA,aAAO;AAACG,eAAOC,KAAKC,KAAL,CAAWL,EAAEM,OAAb,EAAsBH,KAAtB,CAA4BI;AAApC,OAAP;AACH;AACJ;AAxBU,CAAf","sourcesContent":["import FacebookConnection from \"../../connection\";\nimport {Meteor} from \"meteor/meteor\";\n\nMeteor.methods({\n    \"fb-marketing-campaign-create\": function (accountID, adAccount, data) {\n        let account = SocialAccounts.findOne({_id: accountID});\n        let connection = account.connection;\n        let Facebook = FacebookConnection(connection.accessToken);\n\n        let campaign={\n            name: data.name,\n            adlabels:[],\n            budget_rebalance_flag:true,\n            buying_type: data.buyingType, //AUCTION OR RESERVED, FIXED_CPM\n            promoted_object: null, //OBJECT ID\n            spend_cap: Number(data.spendCap)*100,\n            status: \"PAUSED\", //PAUSED OR ACTIVE\n            objective: data.objective //APP_INSTALLS, BRAND_AWARENESS, CONVERSIONS, EVENT_RESPONSES, LEAD_GENERATION, LINK_CLICKS, LOCAL_AWARENESS, OFFER_CLAIMS, PAGE_LIKES, POST_ENGAGEMENT, PRODUCT_CATALOG_SALES, REACH, VIDEO_VIEWS\n        };\n\n        let client = Meteor.wrapAsync(Facebook.napi, Facebook);\n        try{\n            return client(`${adAccount}/campaigns`, \"post\", campaign);\n        }catch(e){\n            console.log(\"ERROR\", e);\n            return {error: JSON.parse(e.message).error.error_user_msg}\n        }\n    }\n});"]},"sourceType":"script","hash":"7fec4191f3038e648e62ec05a1ba2fbd3f64b3c8"}
