Feedback && (\n                    <blockquote className=\"blockquote custom-blockquote blockquote-danger\">\n                        <p className=\"mb-0\">\n                            {ad.reviewFeedback}\n                        </p>\n                    </blockquote>\n                )}\n\n\n\n                <FacebookPreview data={post} showBadge={false}/>\n            </div>\n        )\n    }\n}\n\nexport default AdView;\n\n\n"]},"sourceType":"script","hash":"c562ef9f57796b268b4f79a92f48c24c21a2c9fc"}
