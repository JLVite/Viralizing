;\n\n            li{\n                margin: 5px 10px;\n            }\n        }\n        .list{\n            overflow-x: overlay;\n            display: flex;\n\n            span{\n                margin: 0 !important;\n            }\n            .social-avatar{\n                margin: 5px 10px;\n                display: table;\n            }\n        }\n    }\n    .campaign-actions{\n        .panel{\n            background: #ffffff !important;\n\n            .panel-body{\n                padding: 15px;\n\n                .react-bs-table{\n                    margin: 5px 0;\n                }\n            }\n        }\n    }\n}\n\n.campaign-information{\n    .header{\n        position: relative;\n        margin-bottom: 100px;\n\n        .add{\n            color: Color-Gray;\n        }\n        .cover{\n            background: Color-Soft-Gray;\n            height: 280px\n            text-align: center;\n            cursor: pointer;\n\n            .back{\n                background-size: cover !important;\n                height: 100%;\n                top: 0;\n                left: 0;\n                width: 100%;\n                position: absolute;\n                background-position: center !important;\n            }\n            .add{\n                font-size: 55px;\n                margin-top: 48px;\n\n                &.hidden{\n                    display: none;\n                }\n            }\n        }\n        .profile{\n            background:Color-Soft-Gray;\n            width: 250px;\n            position: absolute;\n            height: 180px;\n            bottom: -87px;\n            left: 45px;\n            text-align: center;\n            cursor: pointer;\n            box-shadow: 0 2px 5px 0 rgba(226,226,226,0.50);\n\n            .back{\n                background-size: cover !important;\n                height: 100%;\n                top: 0;\n                left: 0;\n                width: 100%;\n                position: absolute;\n                background-position: center !important;\n                background-repeat: no-repeat !important;\n            }\n\n            .add{\n                font-size: 50px;\n                margin-top: 50px;\n\n                &.hidden{\n                    display: none;\n                }\n            }\n        }\n\n        .upload-progress{\n            .progress-bar{\n                background: Color-Soft-Gray;\n                position: absolute;\n                width: 100%;\n                height: 100%;\n\n                &.done{\n                    top: -120%;\n                    opacity: 0;\n                }\n\n                .bar {\n                    margin: 5px auto;\n                    font-size: 5px;\n                    position: absolute;\n                    left: 50%;\n                    top: 50%;\n                    margin-top: -40px;\n                    margin-left: -40px;\n                    text-indent: -9999em;\n                    border-top: 1.1em solid rgba(194, 140, 88, 0.2);\n                    border-right: 1.1em solid rgba(194, 140, 88, 0.2);\n                    border-bottom: 1.1em solid rgba(194, 140, 88, 0.2);\n                    border-left: 1.1em solid Color-Copper;\n                    -webkit-transform: translateZ(0);\n                    -ms-transform: translateZ(0);\n                    transform: translateZ(0);\n                    -webkit-animation: load8 1.1s infinite linear;\n                    animation: load8 1.1s infinite linear;\n                    -webkit-transition: opacity 0.7s;\n                    -moz-transition: opacity 0.7s;\n                    -ms-transition: opacity 0.7s;\n                    -o-transition: opacity 0.7s;\n                    transition: opacity 0.7s;\n                    transition-delay top 0.8s;\n\n                    &,&::after{\n                        border-radius: 50%;\n                        width: 80px !important;\n                        height: 80px;\n                    }\n                }\n                @-webkit-keyframes load8 {\n                    0% {\n                        -webkit-transform: rotate(0deg);\n                        transform: rotate(0deg);\n                    }\n                    100% {\n                        -webkit-transform: rotate(360deg);\n                        transform: rotate(360deg);\n                    }\n                }\n                @keyframes load8 {\n                    0% {\n                        -webkit-transform: rotate(0deg);\n                        transform: rotate(0deg);\n                    }\n                    100% {\n                        -webkit-transform: rotate(360deg);\n                        transform: rotate(360deg);\n                    }\n                }\n            }\n        }\n\n        .name{\n            background: Color-White;\n            position: absolute;\n            right: 0;\n            width: calc(100% - 320px);\n            bottom: -100px;\n        }\n    }\n}\n\n\n.campaign-new-message{\n    .post-create{\n        min-height: inherit;\n        .post-write{\n            width: inherit;\n\n            textarea{\n                height: 100px;\n            }\n\n            .image{\n                width: 100px;\n                position: absolute;\n                right: 45px;\n                top: 50px;\n            }\n        }\n    }\n}\n\n\n.campaign-edit-invite{\n\n    .panel{\n        .panel-body {\n            padding: 20px;\n\n            .form-group.no-margin{\n                margin-bottom: 0;\n            }\n        }\n    }\n\n    .messages-list{\n        height: 227px;\n        overflow: auto;\n        overflow-x: hidden;\n\n        .message-item{\n            padding: 10px;\n            display: table;\n\n            .selected{\n                position: absolute;\n                color: Color-Dark-Gray;\n                background: #fff;\n                z-index: 9;\n                border-radius: 50%;\n                width: 30px;\n                height: 30px;\n                font-size: 23px;\n                line-height: 34px;\n                text-align: center;\n                right: 12px;\n                top: 4px;\n                display: none;\n            }\n\n            &.active{\n                .selected{\n                    display: block;\n                }\n            }\n\n            .content{\n                background: Color-Clear-Gray;\n                margin: 0;\n                display: table-cell;\n                vertical-align:middle\n\n                &>div{\n                    display: inline-block;\n                    vertical-align: middle;\n                }\n\n                .description{\n                    width: 66.666667%;\n                    padding: 15px;\n                    text-align: left;\n\n                    .message{\n                        font-size: 0.9em\n                    }\n\n                    .hashtags{\n                        font-size: 1em\n\n                        span{\n                            margin-right: 4px\n                        }\n                    }\n                }\n\n                .media{\n                    width: 33.333333%;\n                    margin: 0;\n\n                    .media-element{\n                        max-width: 100%;\n                        max-height: 100px;\n                        border: 7px solid #fff;\n                    }\n                }\n            }\n        }\n\n        .btn{\n            display: block;\n        }\n    }\n\n\n    .date-panel{\n        .ibol-calendar {\n            .Cal__Container__wrapper{\n                .Cal__Container__listWrapper {\n                    .Cal__List__root{\n                        height: 175px !important;\n                    }\n                }\n            }\n        }\n\n        .date-controls{\n            .time-zone{\n                margin: 12px 0;\n            }\n        }\n    }\n\n\n    .panel{\n        .message-item{\n            .content{\n                background: #fff;\n            }\n        }\n    }\n\n    .btn.save{\n        margin-left: 10px\n    }\n}\n\n\n.invite-view{\n    .invite-scheduled{\n        display: table;\n        width: 100%;\n        height: 687px;\n\n        .content{\n            display: table-cell;\n            vertical-align: middle;\n\n            i{\n                font-size: 7em;\n                margin-bottom: 10px\n            }\n\n            h1{\n                font-size: 2em;\n                margin: 10px 0;\n            }\n\n            p{\n                font-size: 1em;\n            }\n        }\n    }\n\n    .post{\n        .image{\n            margin-bottom: 15px;\n            img{\n                width: 100%;\n                max-width: 200px;\n            }\n        }\n    }\n\n    .post-list{\n        height: 687px;\n        overflow: auto;\n\n        .post{\n            background: rgba(218,218,218,0.1);\n            margin: 10px;\n            border-radius: .286rem;\n            text-align: left;\n            display: block;\n            padding: 20px;\n\n            .social-avatar{\n                margin-right: 20px;\n            }\n\n            .status{\n                display: inline-block;\n                width: inherit;\n                height: inherit;\n                border: none;\n                border-radius: inherit;\n            }\n        }\n    }\n}\n\n\n.invite-actions{\n    h1{\n        margin-top: 5px;\n        margin-bottom: 5px;\n    }\n\n    .controls{\n        ul{\n            list-style: none;\n\n            li{\n                display: inline-block;\n                cursor: pointer;\n                font-size: 1.3rem;\n                height: 2.573rem;\n                padding: 0.35rem 15px;\n                margin-left: 10px;\n            }\n        }\n    }\n\n    .profile-list{\n\n    }\n\n    .panel{\n        .panel-body{\n            padding: 20px;\n        }\n    }\n\n    .list-empty{\n        height: 300px;\n    }\n\n    .social-select{\n        margin-bottom: 4px;\n\n        .Select-control{\n            background: none !important;\n            border: none !important;\n        }\n\n        .social-avatar{\n            width: 50px;\n            height: 50px;\n            margin-right: 3px;\n        }\n\n        .Select-input{\n            height: 50px;\n            line-height: 50px;\n        }\n        .Select-multi-value-wrapper{\n            position: relative;\n\n            &::after{\n                content: \"+\";\n                position: absolute;\n                float: right;\n                width: 50px;\n                height: 50px;\n                right: -38px;\n                top: 0px;\n                z-index: 9;\n                border-radius: 50%;\n                background: #fff;\n                color: Color-Gray;\n                text-align: center;\n                line-height: 50px;\n                font-size: 55px;\n                font-weight: 300;\n                cursor: pointer;\n            }\n        }\n\n        &.is-focused{\n            .Select-multi-value-wrapper{\n                &::after{\n                    display: none;\n                }\n            }\n        }\n\n        &.has-value{\n            .Select-multi-value-wrapper{\n                &::after{\n                    right: -50px;\n                    top: 2px;\n                }\n            }\n        }\n    }\n\n\n    .action-form{\n        .action-post{\n            textarea{\n                width: 100%;\n                height: 173px;\n                border: none;\n                resize: none;\n                background: #fff;\n                margin-bottom: 10px;\n                border-radius: 4px;\n                padding: 20px;\n                padding-right: calc(20% + 20px);\n            }\n\n            .media{\n                width: 17%;\n                position: absolute;\n                right: 30px;\n                top: 10px;\n                padding: 10px;\n\n                img{\n                    width: 100%\n                }\n            }\n        }\n        .drop-zone{\n            width: 100%;\n            border: none;\n            resize: none;\n            background: #fff;\n            padding: 65px;\n            text-align: center;\n            font-size: 1.2em;\n            margin-bottom: 10px;\n            border-radius: 4px;\n\n            i{\n                font-size: 2.2em;\n                display: block;\n                margin-bottom: 8px;\n            }\n        }\n        .controls{\n            padding: 0;\n            margin: 0;\n\n            li{\n                list-style: none;\n                display: inline-block;\n                cursor: pointer;\n                font-size: 24px;\n                margin: 6px;\n                color: Color-Dark-Gray;\n\n                &:hover,\n                &:focus,\n                &.active{\n                    color: Color-Copper;\n                }\n            }\n        }\n        .upload-progress{\n            position: absolute;\n            top: 0;\n            left: 0;\n            width: 100%;\n            .progress-bar{\n                width: 100%;\n                background: none !important;\n                transition: 0.7s all ease-in-out;\n                transition-delay: 0.7s;\n\n                &.done{\n                    opacity:0;\n                }\n\n                .bar{\n                    background: Color-Copper;\n                    transition: 0.7s all ease-in-out;\n                    height: 8px;\n                }\n            }\n        }\n    }\n\n\n    .action-list{\n        .list-group{\n            margin-top: 20px;\n\n            .list-group-item{\n                background: #fff;\n                margin-bottom: 15px;\n                padding: 10px;\n\n                .media-left{\n                    img{\n                        max-height: 85px;\n                        max-width: 150px;\n                    }\n                }\n            }\n        }\n    }\n}\n\n.invite-total{\n    text-align: left;\n\n    table{\n        tr{\n            td:last-child,\n            th:last-child {\n                text-align: right;\n                vertical-align: middle;\n            }\n            .social-avatar{\n                margin-right: 15px;\n            }\n\n            &.bold{\n                td, th{\n                    font-weight: 600;\n                }\n            }\n        }\n    }\n}\n\n.jvectormap-tip{\n    z-index: 9999;\n}\n\n.vectorMap {\n    margin-top: 20px;\n\n    .jvectormap-container{\n        background: none !important;\n    }\n}\n\n\n.campaign-types{\n    a{\n        padding: 0px 10px;\n        color: #76838f;\n        border-right: 1px solid #76838f;\n        &.active{\n            color: #efd600\n        }\n\n        &:hover{\n            text-decoration: none;\n        }\n\n        &:last-child{\n            border-right: none;\n        }\n    }\n}","use('../nodes/vendor-helpers.js')\n@import 'config'\n\n/*\n * Alias \"nowrap\" as \"no-wrap\".\n */\n\nno-wrap = unquote('nowrap')\n\n/*\n * Helper to find out if a given value is a width\n */\n\nis-width(val)\n  if auto == val\n    return true\n  else if val && 'unit' == type(val)\n    // Stylus does not short circuit so we need to perform this as a distinct\n    // operation to prevent errors\n    return '' != unit(val)\n  return false\n\n/*\n * Vendor support for the given prop / arguments, optionally specifying the\n * only prefixes to utilize, or those which should be ignored.\n */\n\nvendor(prop, args, only = null, ignore = null, vendor-property = true)\n  need_normalize = !vendor-property or prop in ('transition' 'transition-property' 'border-image' 'border-image-slice')\n  for prefix in vendor-prefixes\n    unless (only and !(prefix in only)) or (ignore and prefix in ignore)\n      if official == prefix\n        if need_normalize